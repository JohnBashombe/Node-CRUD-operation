{
   "name": "node-crud-operation",
   "version": "1.0.0",
   "description": "This is a simple node CRUD operation in Typescript and SQL",
   "main": "src/app.ts",
   "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "dev": "nodemon --exec ts-node src/app/index.ts",
      "db:create": "typeorm-extension db:create",
      "db:drop": "typeorm-extension db:drop",
      "start": "ts-node src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/JohnBashombe/Node-CRUD-operation.git"
   },
   "keywords": [
      "Backend",
      "ORM",
      "Typescript",
      "node",
      "SQL",
      "Database"
   ],
   "author": "Ntavigwa Bashombe",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/JohnBashombe/Node-CRUD-operation/issues"
   },
   "homepage": "https://github.com/JohnBashombe/Node-CRUD-operation#readme",
   "dependencies": {
      "dotenv": "^16.0.0",
      "express": "^4.17.2",
      "mysql": "^2.14.1",
      "mysql2": "^2.3.3",
      "nodemon": "^2.0.15",
      "ts-node": "^10.5.0",
      "typeorm": "0.2.41",
      "typescript": "^4.5.5"
   },
   "devDependencies": {
      "@types/express": "^4.17.13",
      "@types/node": "^8.0.29",
      "@types/validator": "^13.7.1",
      "ts-node": "3.3.0",
      "typescript": "3.3.3333"
   }
}
